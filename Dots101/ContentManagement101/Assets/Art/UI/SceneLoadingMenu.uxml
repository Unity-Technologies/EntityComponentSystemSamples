    <engine:VisualElement name="menu" class="container" style="visibility: visible; overflow: visible; opacity: 1; scale: 0.85 0.85; transform-origin: right top; margin-top: 5px; margin-right: 5px; margin-bottom: 5px; margin-left: 5px;">
        <engine:Label text="Instructions" class="text" />
        <engine:Label text="When selecting a new option, the previously loaded scene will be unloaded before loading the newly selected scene." style="-unity-font-definition: url(&quot;project://database/Assets/Art/Fonts/ShareTechMono-Regular/ShareTechMono-Regular.ttf?fileID=12800000&amp;guid=ba952712555b503458be5a0ebcad7e60&amp;type=3#ShareTechMono-Regular&quot;); color: rgb(255, 255, 255); font-size: 20px; white-space: pre-wrap;" />
        <engine:Label text="All buttons visually load the same level. However, each option uses a different approach to achieve this.  " style="-unity-font-definition: url(&quot;project://database/Assets/Art/Fonts/ShareTechMono-Regular/ShareTechMono-Regular.ttf?fileID=12800000&amp;guid=ba952712555b503458be5a0ebcad7e60&amp;type=3#ShareTechMono-Regular&quot;); color: rgb(255, 255, 255); font-size: 20px; white-space: pre-wrap;" />
        <engine:VisualElement style="flex-grow: 1;">
